  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.1",
    "config": "^3.3.7",
    "express": "^4.17.2",
    "mongoose": "^6.2.1"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15"
  }
}
